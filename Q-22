#include <stdio.h>
#include<conio.h>

void main()
{
    int i,no1,no2;
    
    printf("Enter the First number : ");
    scanf("%d",&no1);
    
    printf("Enter the Second number : ");
    scanf("%d",&no2);
    
    if(no1>no2)
        i = no1;
    else
        i = no2;
    while(1)
    {
        if (i % no1 == 0 && i % no2 == 0) 
        {
            printf("The LCM of %d and %d is %d.", no1, no2, i);
            break;
        }
        ++i;
    }
    
}

