#include <stdio.h>

void main()
{
    int k,num,i,j;
    printf("Enter the number : ");
    scanf("%d",&num);
    if(num > 2){
    printf("*");
    for(j=0;j<num-1;j++)
        {
            printf("  ");
        }
    for(i=0;i<num;i++)
        {
            printf("* ");
        }
    printf("\n");
    
    for(i=0;i<num-2;i++)
    {
      printf("*");
        for(j=0;j<num-1;j++)
        {
            printf("  ");
        }
        printf("*\n");
    }
    for(i=1;i<=num*2;i++)
    {
        printf("* ");
    }
    printf("\n");
    for(i=0;i<num-2;i++)
    {
        printf(" ");
        for(j=0;j<num-1;j++)
        {
            printf("  ");
        }
        printf("*");
        for(j=0;j<num-1;j++)
        {
            printf("  ");
        }
        printf("*\n");
    }
    
        for(j=0;j<num;j++)
        {
            printf("* ");
        }
        for(j=1;j<num;j++)
        {
            printf("  ");
        }
        printf("*");
    }
    else 
    return;

}

