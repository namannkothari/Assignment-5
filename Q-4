#include<stdlib.h>
#include <stdio.h>

void main()
{
    int i,ans=1,a;
    
    printf("Enter the number whose factorial you want:");
    scanf("%d",&a);
    
    for(i=1;i<=a;i++){
        
        ans=ans*i;
    }
    
    printf("The factorial of the number is: %d",ans);
    
}
